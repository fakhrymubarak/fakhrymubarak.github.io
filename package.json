{
  "name": "fakhrymubarak.github.io",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build-with-version.cjs && tsc && vite build",
    "build:analyze": "node scripts/build-with-version.cjs && tsc && vite build --mode analyze",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0 --ignore-pattern 'src/domain/valueObjects/sw.ts'",
    "preview": "vite preview",
    "test": "jest",
    "test:fast": "jest --no-coverage --testPathIgnorePatterns='.*accessibility\\.test\\.tsx' --maxWorkers=4",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:accessibility": "jest --testPathPattern=accessibility.test",
    "test:performance": "jest --testPathPattern=performance.test",
    "test:e2e": "playwright test",
    "test:all": "npm run test:coverage && npm run test:accessibility && npm run test:performance",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "lighthouse": "lhci autorun",
    "deploy": "bash deploy.sh"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "firebase": "^12.1.0",
    "framer-motion": "^10.16.5",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.20.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "audit-ci": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "canvas": "^3.1.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.4.0",
    "typescript": "^5.2.2",
    "vite": "^7.1.9",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-compression": "^0.5.1"
  }
}
